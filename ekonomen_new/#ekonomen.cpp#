#include <sqlite3.h>

using namespace std;

static int callback(void *NotUsed, int argc, char **argv, char **azColName) {
	int i;
	for (i = 0; i<argc; i++) {
		cout<<azColName[i]<<" = "
	}
}

int main() {
	const string DBname = "ekonomen.db";
	sqlite3 *db;
	rc = sqlite3_open(DBname,&db);

	string sql = ".tables";

	rc = sqlite3_exec(db,sql)

	sqlite3_close(db_)
	return 0;
}